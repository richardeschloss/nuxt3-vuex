{
  "name": "nuxt3-vuex",
  "version": "1.0.0",
  "description": "Vuex support for Nuxt3",
  "main": "lib/module.js",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --watchAll",
    "test:cov": "c8 npm test"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "nuxt3",
    "vuex",
    "store"
  ],
  "author": "Richard Schloss",
  "license": "MIT",
  "dependencies": {
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "c8": "^7.12.0",
    "glob": "^8.0.3",
    "jest": "^29.0.1",
    "nuxt": "npm:nuxt3@latest"
  }
}
